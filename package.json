{
  "name": "zenitsu",
  "version": "0.0.1",
  "description": "A bot for entertainment",
  "author": "MARCROCK22",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "build": "npx swc ./src -d dist",
    "runall": "concurrently \"node ./dist/request/run.js\" \"node ./dist/gateway/run.js\" \"node ./dist/bot/run.js\" \"node ./dist/database/run.js\" \"node ./dist/images/run.js\" -c \"green,red,blue,yellow,cyan\" -p \"[{name}]\" -n \"HANDLER,GATEWAY,BOT,DATABASE,IMAGES\"",
    "test-swc": "npx nodemon --exec \"swc ./src -d dist && npm run runall\"",
    "pm2:request": "pm2 start ./dist/request/run.js --name request",
    "pm2:gateway": "pm2 start ./dist/gateway/run.js --name gateway",
    "pm2:bot": "pm2 start ./dist/bot/run.js --name bot",
    "pm2:database": "pm2 start ./dist/database/run.js --name database",
    "pm2:images": "pm2 start ./dist/images/run.js --name images",
    "pm2": "npm run pm2:database && npm run pm2:images && npm run pm2:request && npm run pm2:bot && npm run pm2:gateway",
    "pm2:stop": "pm2 stop all",
    "restart:pm2": "pm2 delete all && npm run pm2"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@sapphire/async-queue": "^1.3.1",
    "detritus-client-rest": "github:detritusjs/client-rest#0.10.6",
    "detritus-client-socket": "github:detritusjs/client-socket#0.8.4",
    "discord-api-types": "^0.30.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "imagescript": "github:matmen/ImageScript",
    "ioredis": "^5.0.4",
    "kufa": "^1.6.5",
    "multer": "^1.4.4",
    "node-fetch": "^3.2.4",
    "nodemon": "^2.0.16",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.0",
    "websocket": "^1.0.34",
    "ws": "^8.6.0",
    "zod": "^3.15.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.177",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node-fetch": "^2.6.1",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.15.0",
    "pm2": "^5.2.0",
    "prisma": "^3.14.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
}